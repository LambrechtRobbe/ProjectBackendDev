@model WickedQuiz.Models.Models.Score

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ResultQuiz</title>
</head>
<body>

<div>
    <h4>Score</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FinalScore)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FinalScore)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.MaxScore)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MaxScore)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DateFinished)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DateFinished)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ApplicationUserId)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ApplicationUserId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.QuizId)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.QuizId)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Quizzes">Back to quizzes</a>
</div>
</body>
</html>
